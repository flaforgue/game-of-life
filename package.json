{
  "name": "game-of-life",
  "version": "1.0.0",
  "repository": "git@github.com:flaforgue/game-of-life.git",
  "author": "Florian Laforgue <florian.laforgue@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "install:lock": "yarn --frozen-lockfile",
    "format:check": "tsc && eslint \"./src/**\"",
    "format:fix": "tsc && eslint --fix \"./src/**\"",
    "test": "jest",
    "dev": "yarn install:lock && vite dev --host",
    "start": "yarn install:lock && vite build && vite preview --host",
    "build": "yarn install:lock && vite build"
  },
  "engines": {
    "node": "^22.14.0",
    "yarn": "^1.22.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@stylistic/eslint-plugin-js": "^4.2.0",
    "@stylistic/eslint-plugin-ts": "^4.2.0",
    "@types/jest": "^29.5.14",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.0",
    "vite": "^6.0.4"
  }
}